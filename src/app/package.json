{
  "name": "sxastarter",
  "description": "Application utilizing Sitecore JavaScript Services and Next.js",
  "version": "21.1.4",
  "private": true,
  "config": {
    "appName": "app",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en",
    "templates": [
      "nextjs",
      "nextjs-sxa",
      "nextjs-personalize",
      "nextjs-multisite"
    ]
  },
  "engines": {
    "node": "18.x",
    "npm": ">=6"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@coveo/atomic-react": "^2.8.10",
    "@sitecore-feaas/clientside": "^0.3.10",
    "@sitecore-jss/sitecore-jss-nextjs": "^21.2.4",
    "@sitecore/engage": "^0.4.0",
    "@splidejs/react-splide": "^0.7.12",
    "@tanstack/react-query": "^4.28.0",
    "@types/nodemailer": "^6.4.7",
    "a11y-nav": "^1.4.0",
    "a11y-slider": "^0.4.22",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "focus-overlay": "^1.0.6",
    "font-awesome": "^4.7.0",
    "graphql": "~15.8.0",
    "graphql-tag": "^2.12.6",
    "next": "^13.1.6",
    "next-localization": "^0.12.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.1",
    "nprogress": "^0.2.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.45.4",
    "react-player": "^2.11.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.2",
    "react-transition-group": "^4.4.5",
    "ts-deepmerge": "^6.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/plugin-helpers": "^3.1.2",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@sitecore-jss/sitecore-jss-cli": "^21.1.4",
    "@sitecore-jss/sitecore-jss-dev-tools": "~21.2.4",
    "@svgr/webpack": "^6.5.1",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-transition-group": "^4.4.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "axios": "^0.21.1",
    "chalk": "~4.1.2",
    "chokidar": "~3.5.3",
    "constant-case": "^3.0.4",
    "cross-env": "~7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-yaml": "^0.5.0",
    "graphql-let": "^0.18.4",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "~4.1.5",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "sass": "^1.52.3",
    "sass-alias": "^1.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "~4.9.4",
    "yaml-loader": "^0.8.0"
  },
  "scripts": {
    "jss": "jss",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts ./scripts/**/*.ts",
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "build": "npm-run-all --serial bootstrap next:build",
    "graphql:update": "ts-node --project tsconfig.scripts.json ./scripts/fetch-graphql-introspection-data.ts",
    "next:build": "next build",
    "next:dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "next:start": "next start",
    "next:lint": "next lint",
    "scaffold": "ts-node --project tsconfig.scripts.json scripts/scaffold-component.ts",
    "start": "npm run start:connected",
    "start:connected": "npm-run-all --serial bootstrap --parallel next:dev start:watch-components",
    "start:production": "npm-run-all --serial bootstrap next:build next:start",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-builder/index.ts --watch",
    "prepare": "cd ../../ && husky install src/app/.husky",
    "check-types": "tsc --pretty --noEmit"
  }
}
